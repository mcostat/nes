{
  "[django-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html][django-html][handlebars][hbs][mustache][jinja][jinja-html][nj][njk][nunjucks][twig]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  "djlint.enableLinting": false,
  "djlint.extendExclude": [
    "*.py"
  ],
  "djlint.formatAttributeTemplateTags": false,
  "djlint.ignore": [
    "D018",
    "H031"
  ],
  "djlint.lineBreakAfterMultilineTag": true,
  "djlint.maxAttributeLength": 160,
  "djlint.maxBlankLines": 1,
  "djlint.maxLineLength": 180,
  "djlint.profile": "django",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "mypy-type-checker.preferDaemon": true,
  "mypy-type-checker.reportingScope": "workspace",
  "prettier.printWidth": 120,
  "pylint.importStrategy": "fromEnvironment",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.exclude": [
    "**/docs/",
    ".git",
    "**/migrations",
    "**/tests"
  ],
  "python.analysis.extraPaths": [
    "${localWorkspaceFolder}",
    "patientregistrationsystem/qdc"
  ],
  "python.analysis.include": [],
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.packageIndexDepths": [
    {
      "depth": 2,
      "includeAllSymbols": true,
      "name": "django"
    }
  ],
  "python.analysis.typeCheckingMode": "off",
  "python.languageServer": "Default"
}
