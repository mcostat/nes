{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "python.analysis.autoImportCompletions": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.languageServer": "Default",
    "python.analysis.typeCheckingMode": "off",
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.autoSearchPaths": true,
    "python.analysis.fixAll": [
        "source.convertImportFormat",
        "source.unusedImports"
    ],
    "python.analysis.packageIndexDepths": [
        {
            "name": "django",
            "depth": 10,
            "includeAllSymbols": true
        }
    ],
    "pylint.args": [
        "--disable=C0111",
        "--load-plugins",
        "pylint_django"
    ],
    "python.analysis.include": [],
    "python.analysis.exclude": [
        "**/docs/",
        ".git",
        "**/migrations",
        "**/tests"
    ],
    "python.autoComplete.extraPaths": [
        "${localWorkspaceFolder}, /usr/local/bin/python"
    ],
    "python.analysis.extraPaths": [
        "${localWorkspaceFolder}",
        "patientregistrationsystem/qdc"
    ],
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "mypy-type-checker.reportingScope": "workspace",
    "mypy-type-checker.preferDaemon": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./patientregistrationsystem/qdc",
        "-p",
        "test*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "[html][django-html][handlebars][hbs][mustache][jinja][jinja-html][nj][njk][nunjucks][twig]": {
        "editor.defaultFormatter": "monosans.djlint"
    },
    "djlint.enableLinting": false,
    "djlint.maxLineLength": 180,
    "djlint.useGitignore": true,
    "djlint.lineBreakAfterMultilineTag": true,
    "djlint.maxBlankLines": 1,
    "djlint.formatAttributeTemplateTags": false,
    "djlint.maxAttributeLength": 160,
    "djlint.blankLineAfterTag": [
        "endblock",
        "include",
        "extends",
        "load"
    ],
    "djlint.blankLineBeforeTag": [
        "block",
        "include",
        "load"
    ],
    "djlint.profile": "django",
    "djlint.ignore": [
        "D018",
        "H031"
    ],
    "djlint.extendExclude": [
        "*.py"
    ],
    "[django-html]": {
        "editor.defaultFormatter": "monosans.djlint"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true
    },
    "editor.formatOnSave": true,
    "prettier.printWidth": 120
}